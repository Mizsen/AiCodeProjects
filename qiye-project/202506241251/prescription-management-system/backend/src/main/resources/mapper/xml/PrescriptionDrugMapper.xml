<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.prescription.mapper.PrescriptionDrugMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.prescription.entity.PrescriptionDrugEntity">
        <id column="id" property="id" />
        <result column="prescription_id" property="prescriptionId" />
        <result column="drug_id" property="drugId" />
        <result column="dosage" property="dosage" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, prescription_id, drug_id, dosage
    </sql>

</mapper>
